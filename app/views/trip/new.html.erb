<h4><%=@location %></h4>
<h4>Climbing route options for your Trip!</h4>
<% climb_results.each do |climb| %>
  <section class="climbs">
    <h5><%= climb.name %></h5>
    <p><%= climb.climb_type %></p>
    <p><%= climb.rating %></p>
    <p><%= climb.stars %></p>
    <p><%= climb.pitches %></p>
    <p><%= climb.location %></p>
    <p><%= link_to climb.url, climb.url, target: :_blank %></p>
  </section>
<% end %>